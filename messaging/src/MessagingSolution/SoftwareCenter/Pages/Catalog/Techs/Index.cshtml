@page
@model SoftwareCenter.Pages.Catalog.Techs.IndexModel
@{
    
}

@if (Model.Techs is not null)
{
    <p>Some Techs</p>
        <ul class="list-group list-group-flush">
    @foreach (var tech in Model.Techs)
    {
            <li class="list-group-item">@tech.Name</li>
       
    }
        </ul>
}
        
<div>
    <form method="post">
        <div class="form-group">
            <label asp-for="TechModel.Name"></label>
            <input asp-for="TechModel.Name" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="TechModel.EmailAddress"></label>
            <input asp-for="TechModel.EmailAddress" class="form-control"/>
        </div>
        <button type="submit" class="btn btn-primary">Add Support Tech</button>

    </form>
</div>